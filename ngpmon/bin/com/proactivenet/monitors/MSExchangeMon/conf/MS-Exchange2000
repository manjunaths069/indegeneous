--
object=monitor,RoundTripEmail
groups=
options=
MSGSIZE=5
POPHOST=$(POP3 Server/Round Trip Email Monitor)
POPPASSWORD=$(Recievers POP3 Password/Round Trip Email Monitor)
POPUSER=$(Recievers E-Mail Id/Round Trip Email Monitor)
SMTPUSER=$(Senders E-Mail Id/Round Trip Email Monitor)
SRCIP=$(DEVICE_AGENT_NAME)
COLLECTDATA=true
RETRY_COUNT=1
RETRY_DELAY=20
STSPOLLPERIOD=300
STSPOLLTIMEOUT=40
