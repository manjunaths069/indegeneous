#Template name = MS Exchange Template
object=monitor,DNS
groups=
options=
HOST=$(DomainName/IP to lookup for DNS monitor)
PORT=53
SRCIP=$(DEVICE_AGENT_NAME)
TARGETIP=$(MS-ExchangeServer IP)
COLLECTDATA=true
RETRY_COUNT=3
RETRY_DELAY=20
STSPOLLPERIOD=300
STSPOLLTIMEOUT=20
