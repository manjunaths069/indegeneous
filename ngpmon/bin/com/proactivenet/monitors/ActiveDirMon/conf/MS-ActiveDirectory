#Template name = Active Directory Template
object=monitor,LDAPQuery
groups=
options=
BASE=$(Base for LDAP Monitor)
BIND=$(Bind for LDAP Monitor)
INSTANCE_NAME=LDAP
PASSWORD=$(Password for LDAP Monitor)
PORT=389
FILTER=(objectclass=*)
SCOPE=One
SRCIP=$(DEVICE_AGENT_NAME)
TARGETIP=$(Domain Controller IP)
COLLECTDATA=true
STSPOLLPERIOD=300
STSPOLLTIMEOUT=120
--
